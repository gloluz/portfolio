{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build",
    "dev": "concurrently -n \"building,server\" \"npm run dev:build\" \"npm run dev:server\"",
    "dev:build": "webpack --watch",
    "dev:server": "cross-env NODE_ENV=development && nodemon dist/server.js",
    "prod:build": "cross-env NODE_ENV=production && webpack",
    "prod:server": "node dist/server.js",
    "build": "npm run prod:build && npm run prod:server"
  },
  "dependencies": {
    "@types/styled-components": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "icomoon-react": "^2.0.16",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-lazy-images": "^1.1.0",
    "react-lazy-load-image-component": "1.3.2",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.4",
    "@types/node": "^13.11.0",
    "@types/react": "^16.9.32",
    "@types/react-dom": "^16.9.6",
    "@types/react-lazy-load-image-component": "^1.3.0",
    "@types/react-router-dom": "^5.1.3",
    "awesome-typescript-loader": "^5.2.1",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "file-loader": "^6.0.0",
    "json-loader": "^0.5.7",
    "nodemon": "^2.0.2",
    "style-loader": "^1.1.3",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": "13.8.x"
  }
}
